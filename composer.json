{
  "name": "surender/prodege-api",
  "description": "Laravel package for Prodege MR API integration",
  "type": "library",
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^12.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Surender\\ProdegeApi\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Surender Rawat",
      "email": "surenderrawatofficial@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Surender\\ProdegeApi\\ProdegeApiServiceProvider"
      ],
      "aliases": {
        "ProdegeApi": "Surender\\ProdegeApi\\Facades\\ProdegeApi"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
